{
	"compilerOptions": {
		"lib": [
			"ESNext",
			"DOM"
		],
		"target": "ESNext",
		"module": "ESNext",
		"moduleDetection": "force",
		"jsx": "react-jsx",
		"jsxImportSource": "react",
		"allowJs": true,
		"moduleResolution": "bundler",
		"allowImportingTsExtensions": true,
		"verbatimModuleSyntax": true,
		"noEmit": true,
		"strict": true,
		"skipLibCheck": true,
		"noFallthroughCasesInSwitch": true,
		"noUnusedLocals": false,
		"noUnusedParameters": false,
		"noPropertyAccessFromIndexSignature": false,
		"types": [
			"temporal-polyfill",
			"react-dom",
			"react",
			"bun"
		],
		"baseUrl": "./",
		"paths": {
			"@/*": [
				"./*"
			]
		}
	},
	"include": [
		"gyoza-env.d.ts",
		"**/*.tsx",
		"**/*.ts",
		".config/build.browser-client.ts",
		".config/build.bun-server.ts",
		".config/build.ts"
	]
}