{
	"name": "@gyoza/gyoza-react",
	"private": true,
	"description": "Gyoza is an experimental framework for React applications powered by bun and elysia.",
	"license": "MIT",
	"version": "0.1.0",
	"author": {
		"name": "d-spindola"
	},
	"type": "module",
	"main": "server.js",
	"scripts": {
		"dev": "bun run --port=3000 --watch ./server.ts",
		"format": "bunx @biomejs/biome format --write ./",
		"clean": "rm -rf ./dist ./node_modules",
		"start": "bun --port=8080 ./server.ts"
	},
	"overrides": {
		"@types/react": "npm:types-react@rc",
		"@types/react-dom": "npm:types-react-dom@rc"
	},
	"dependencies": {
		"@elysiajs/cors": "^1.0.2",
		"@elysiajs/eden": "^1.0.14",
		"@elysiajs/static": "^1.0.3",
		"@elysiajs/swagger": "^1.0.5",
		"@elysiajs/websocket": "^0.2.8",
		"@types/bun": "latest",
		"@types/react": "npm:types-react@rc",
		"@types/react-dom": "npm:types-react-dom@rc",
		"@vitejs/plugin-react-swc": "^3.7.0",
		"classnames": "^2.5.1",
		"elysia": "^1.0.23",
		"react": "^19.0.0-rc-dfd30974ab-20240613",
		"react-dom": "^19.0.0-rc-dfd30974ab-20240613"
	},
	"keywords": ["bun-js", "elysia", "rsc", "react", "experimental"],
	"repository": {
		"url": "https://github.com/no-turno/gyoza-react"
	},
	"peerDependencies": {
		"typescript": "latest"
	}
}
